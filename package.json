{
  "name": "log-owl",
  "version": "0.1.0",
  "productName": "Log Owl",
  "description": "Desktop time tracking application",
  "main": "dist-electron/main/main.js",
  "scripts": {
    "dev": "pnpm run build:electron && concurrently \"pnpm run dev:vite\" \"pnpm run dev:electron\"",
    "dev:vite": "vite",
    "dev:electron": "wait-on http://localhost:5173 && electron .",
    "preview": "vite preview",
    "build": "pnpm run build:desktop",
    "build:desktop": "pnpm run build:electron && vite build && electron-builder",
    "build:desktop:mac": "pnpm run build:electron && vite build && electron-builder --x64 --arm64",
    "build:electron": "pnpm run generate-preload-channels && tsc -p main && tsc-alias -p main/tsconfig.json",
    "typecheck": "tsc -p main && tsc -p renderer/tsconfig.json --noEmit",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "format": "prettier --write .",
    "format:check": "prettier --check .",
    "spellcheck": "cspell lint",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:coverage": "vitest run --coverage",
    "pree2e": "pnpm run build:electron",
    "e2e": "playwright test",
    "check": "pnpm run lint && pnpm run typecheck && pnpm run format:check && pnpm run spellcheck && pnpm run test:coverage && pnpm run e2e",
    "analyze:bundle": "ANALYZE=true vite build",
    "analyze:exports:main": "ts-unused-exports main/tsconfig.json",
    "analyze:exports:renderer": "ts-unused-exports renderer/tsconfig.json",
    "generate-preload-channels": "node scripts/generate-preload-channels.js",
    "postinstall": "electron-rebuild -f -w better-sqlite3",
    "prepare": "husky"
  },
  "packageManager": "pnpm@10.30.1",
  "lint-staged": {
    "*.{ts,tsx,js,jsx,mjs,cjs}": [
      "eslint --fix",
      "prettier --write"
    ],
    "*.{json,md,html}": [
      "prettier --write"
    ]
  },
  "pnpm": {
    "onlyBuiltDependencies": [
      "electron",
      "better-sqlite3"
    ]
  },
  "keywords": [],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "@emotion/react": "^11.11.3",
    "@emotion/styled": "^11.11.0",
    "@mui/icons-material": "^5.15.3",
    "@mui/material": "^5.15.3",
    "better-sqlite3": "^11.0.0",
    "dayjs": "^1.11.10",
    "react-hook-form": "^7.71.2",
    "react-router": "^7.13.1",
    "zod": "^4.3.6",
    "zustand": "^4.4.7"
  },
  "devDependencies": {
    "@commitlint/cli": "^20.4.2",
    "@commitlint/config-conventional": "^20.4.2",
    "@eslint/js": "^9.39.3",
    "@playwright/test": "^1.58.2",
    "@testing-library/jest-dom": "^6.9.1",
    "@testing-library/react": "^16.3.2",
    "@testing-library/user-event": "^14.6.1",
    "@types/better-sqlite3": "^7.6.11",
    "@types/node": "^20.10.0",
    "@types/react": "^18.2.43",
    "@types/react-dom": "^18.2.17",
    "@vitejs/plugin-react": "^4.2.1",
    "@vitest/coverage-v8": "^1.6.1",
    "concurrently": "^8.2.2",
    "cspell": "^9.7.0",
    "cspell-dict-ru_ru": "^1.4.0",
    "electron": "^28.3.3",
    "electron-builder": "^24.9.1",
    "electron-rebuild": "^3.2.9",
    "eslint": "^9.39.3",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-import": "^2.32.0",
    "eslint-plugin-react": "^7.37.5",
    "eslint-plugin-react-hooks": "^7.0.1",
    "eslint-plugin-simple-import-sort": "^12.1.1",
    "globals": "^17.3.0",
    "husky": "^9.1.7",
    "jsdom": "^28.1.0",
    "lint-staged": "^16.2.7",
    "prettier": "^3.4.2",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "rollup-plugin-visualizer": "^7.0.0",
    "ts-unused-exports": "^11.0.0",
    "tsc-alias": "^1.8.16",
    "typescript": "^5.3.3",
    "typescript-eslint": "^8.56.0",
    "vite": "^5.0.8",
    "vitest": "^1.6.1",
    "wait-on": "^7.2.0"
  }
}
